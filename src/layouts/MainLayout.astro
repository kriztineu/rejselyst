---
import "../styles/style.css"
import "../styles/stylefonts.css"
import "../styles/layout.css"
const { title } = Astro.props;
import Header from "../components/general/Header.astro";
import Footer from "../components/general/Footer.astro";
---

<html lang="da">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=Be+Vietnam+Pro:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
        <meta
		name="description"
		content="Rejselyst er et rejsebureau ejet af Matti og Tina. De specialiserer sig i skræddersyede rejser til Maldiverne og andre luksus destinationer og hoteller, håndplukket og testet af dem selv. "
	    />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body>
        <Header/>
		<slot/>
        <Footer/>
	</body>
</html>
